    "@turbo/gen": "^1.13.4",
    "@types/node": "^20.14.5",
    "@vitest/browser": "^1.6.0",
    "jsdom": "^24.1.0",
    "playwright": "^1.44.1",
    "prettier": "^3.3.2",
    "syncpack": "^12.3.2",
